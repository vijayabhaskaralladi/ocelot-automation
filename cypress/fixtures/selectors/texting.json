{
  "texting": {
    "campaignAnalytics": {
      "contactsMessaged": "[data-testid='Contacts Messaged-counter']",
      "contactsResponded": "[data-testid='Contacts Responded-counter']",
      "contactResponsesPerHourChart": { "selector": ".MuiCardContent-root>.chartjs-render-monitor", "index": 0 },
      "exportAnalytics": "button[aria-label=' Export results to CSV']",
      "filter": "button[aria-label='Filter results']",
      "labelOffice": { "selector":"label", "text": "Office" },
      "officeStatus": { "selector":"div[class*='MuiInput-input']", "index": 1 }
    },
    "activeCampaigns": {
      "viewFirstRow": { "selector": "[role='group']>button:nth-child(1)", "index": 0 },
      "generalTab": { "selector": "span.MuiTab-wrapper", "text": "General" },
      "campaignName": { "selector": "div.MuiFormControl-fullWidth>div", "index": 0 },
      "keywordSearch": "input[name='keywords']",
      "responseInput": "[aria-label='Rich Text Editor, main']",
      "filterResults": "svg[title='Filter results']",
      "statusFilter": {"selector": "input[name='status']", "getPrevious": true },
      "draftFilter": "li[data-value='Draft']",
      "draftFilterName": "div[title='Status: Draft']>span",
      "campaginListTable": "table[data-testid='campaignListTable']>tbody>tr",
      "firstContactNumber": { "selector": "td:nth-child(4)", "index": 0 },
      "exportResults": "button[title=' Export results to CSV']",
      "firstRowDropdown": { "selector": "div[role='group']>button:nth-child(2)", "index": 0 },
      "cloneButton": { "selector": "span>li", "text": "Clone" },
      "campaignNameInputDialog": "input[aria-invalid='true']",
      "confirmClone": { "selector": ".MuiDialogActions-root>button>span", "text": "Clone" }
    },
    "phoneNumbers": {
      "phoneNumberFromFirstRow": { "selector": "tbody>tr>td>div.MuiBox-root", "index": 0 },
      "exportResults": "button[aria-label=' Export results to CSV']",
      "searchInput": "input[name='keywords']",
      "records": "tbody.MuiTableBody-root>tr",
      "archiveDate": "tr>td:nth-child(8)",
      "editPhoneNumberButton": "button[title='Edit phone number']",
      "phoneNumberDescription": "#description",
      "filter": "button[aria-label='Filter results']",
      "labelNumberType": { "selector":"label", "text": "Number Type" },
      "inputNumberType": { "selector":"div[class*='MuiInput-input']", "index": 2 },
      "rowSelector": "tr[class='MuiTableRow-root']"
    },
    "transcripts": {
      "readStatusButtonFirstRow": { "selector": "td.MuiTableCell-alignCenter>button", "index": 0 },
      "exportTranscripts": "button[aria-label=' Export results to CSV']",
      "viewConversationFirstRow": { "selector": "td button.MuiButtonBase-root", "index": 0 },
      "conversationTab": { "selector": "h6", "text": "Conversation Details" },
      "searchInput": "input[name='keywords']",
      "filterResults": "svg[title='Filter results']",
      "readStatusFilter": {"selector": "input[name='readStatus']", "getPrevious": true },
      "readFilter": "li[data-value='Read']",
      "unReadFilter": "li[data-value='Unread']",
      "transcriptListTable": "tbody.MuiTableBody-root>tr",
      "unreadFilterName": "div[title='Read Status: Unread']"
    },
    "archived": {
      "firstRowDropdown": { "selector": "div[role='group']>button:nth-child(2)", "index": 0 },
      "cloneButton": { "selector": "span>li", "text": "Clone" },
      "campaignNameInputDialog": "input[aria-invalid='true']",
      "confirmClone": { "selector": ".MuiDialogActions-root>button>span", "text": "Clone" },
      "archivedRowCount": "table[aria-label='Campaigns'] tr",
      "firstViewButton": { "selector": "tr span[class='MuiButton-label']", "index": 0 },
      "contactsRowCount": { "selector": "table[aria-label='Contacts'] tr", "index": 0 },
      "contactsMessaged": "span[data-testid='Contacts Messaged-counter']",
      "contactsResponded": "span[data-testid='Contacts Responded-counter']"
    }
  }
}
