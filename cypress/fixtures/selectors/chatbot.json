{
  "chatbot": {
    "analytics": {
      "conversationsNumber": "div>div:nth-child(1)>div.MuiTypography-root.MuiTypography-h3",
      "interactionsNumber": "div>div:nth-child(1)>div.MuiTypography-root.MuiTypography-subtitle1"
    },
    "transcripts": {
      "viewConversationFirstRow": { "selector": "td>button.MuiButtonBase-root.MuiIconButton-root", "index": 0 },
      "closeConversation": "button[aria-label*='Close']",
      "markReadFirstRow": { "selector": "[title='Mark read']", "index": 0 },
      "markUnreadFirstRow": { "selector": "[title='Mark unread']", "index": 0 },
      "exportTranscripts": "button[aria-label=' Export results to CSV']"
    },
    "interactions": {
      "exportInteractions": "button[aria-label=' Export results to CSV']"
    },
    "knowledgebase": {
      "generalLibrary": {
        "questions": ".MuiButtonBase-root.MuiAccordionSummary-root",
        "viewFirstQuestion": { "selector": "div.MuiAccordionSummary-expandIcon", "index": 0 },
        "search": "input[name='keywords']",
        "answer": {"selector":".MuiAccordionDetails-root p","index": 0},
        "buttonContentShare": {"selector": "div.MuiTypography-root","text":"Shared Content"},
        "buttonSave": {"selector":"span","text":"Save"},
        "msgSuccess": {"selector": "div[id='notistack-snackbar']","text": "Your question has been successfully published and will soon become part of the bot's knowledgebase!"},
        "contentShare": "svg[title='Shared with other bot(s)']"
      },
      "customQuestions": {
        "questions": ".MuiButtonBase-root.MuiAccordionSummary-root",
        "search": "input[name='keywords']",
        "viewFirstQuestion": { "selector": "div.MuiAccordionSummary-expandIcon", "index": 0 },
        "exportCustomQuestions": "button[aria-label=' Export results to CSV']",
        "buttonContentShare": {"selector": "div.MuiTypography-root","text":"Shared Content"},
        "buttonSave": {"selector":"span","text":"Save"},
        "msgSuccess": {"selector": "div[id='notistack-snackbar']","text": "Your question has been successfully published and will soon become part of the bot's knowledgebase!"},
        "contentShare": "svg[title='Shared with other bot(s)']",
        "questionCloseButton": "button[aria-label='close']",
        "contentSharingCheckbox": "#contentShare",
        "questionSaveButton": ".MuiButtonBase-root.MuiButton-root.MuiButton-contained.MuiButton-containedPrimary",
        "questionTable": "div.MuiButtonBase-root.MuiAccordionSummary-root",
        "selectedQuestion": "div:nth-child(2)>div.MuiAccordionSummary-root>div>p"
      },
      "questionTemplates": {
        "search": "input[name='keywords']",
        "viewFirstQuestion": { "selector": "div.MuiAccordionSummary-expandIcon", "index": 0 },
        "searchQuestionAnswer": "div[role='region']>div.MuiAccordionDetails-root"
      },
      "moderationQueue": {
        "cellsWithQuestion": "th[role='cell']",
        "exportModerationQueue": "button[aria-label=' Export results to CSV']",
        "firstRowQuestion": "tbody>tr:nth-child(1)>th"
      }
    },
    "lookAndFeel": {
      "welcomeMessageInput": "div[aria-label='Rich Text Editor, main']"
    },
    "variables": {
      "requiredNumber": { "selector": "div.MuiTypography-caption", "index": 0 },
      "optionalNumber": { "selector": "div.MuiTypography-caption", "index": 1 }
    },
    "feedback": {
      "netPromoterScoreValue": { "selector": "svg>text", "index": 1 }
    },
    "embedding": {
      "scriptText": ".language-htmlBars>span>span"
    },
    "spider": {
      "spiderTable": ".MuiTable-root>tbody>tr",
      "viewFirstSpiderButton": { "selector": "button[title='View Content']", "index": 0 }
    }
  }
}

