      "variableRichTextValue": "div[aria-label='Rich Text Editor, main'],[aria-multiline='true']",
      "variableLinkTextValue": "input[placeholder='Our Homepage']",
      "variableLinkURLValue": "input[placeholder='https://www.ocelotbot.com/']",
      "editAllowedValuesButton": "button[data-testid='editAllowedValuesButton']",
      "dropdownOptionValues": "div.MuiInputBase-root input[data-key='0']",
