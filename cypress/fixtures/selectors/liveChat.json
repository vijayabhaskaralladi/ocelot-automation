{
  "liveChat": {
    "activeConversations": {
      "inboundDialog": { "selector": "p", "text": "Inbound (Unassigned)" },
      "latestConversation": { "selector": "li>div.MuiButtonBase-root", "index": 0 },
      "input": "div[aria-label='Rich Text Editor, main']",
      "send": "button.MuiButtonBase-root[type='submit']"
    },
    "analytics": {
      "chatsNumber": { "selector": "div.MuiTypography-colorPrimary", "index": 0 },
      "missedChatsNumber": { "selector": "div.MuiTypography-h5", "index": 0 },
      "averagePerDayMissedChatsNumber": { "selector": "div.MuiTypography-h5", "index": 1 }
    },
    "whosOnline": {
      "agentsOnline": { "selector": ".MuiGrid-item>p", "index": 1 },
      "totalAgents": { "selector": ".MuiGrid-item>p", "index": 3 }
    },
    "cannedResponses": {
      "searchInput": "input[name='keywords']",
      "firstCannedResponseItem": { "selector": "p[data-testid='responseTitle']", "index": 0},
      "deleteButton": { "selector": "span", "text": "Delete" },
      "confirmDelete": "div[role='dialog']>div>span>button>span",
      "successDeletionNotification": { "selector": "#notistack-snackbar", "text": "Your response has been deleted." },
      "createButton": "button[aria-label='Add New Canned Response']",
      "titleInput": "input[aria-label='Title']",
      "responseText": "div[aria-label='Rich Text Editor, Response']",
      "tagsInput": "input[aria-labelledby='canned-response-tags']",
      "saveButton": "button.MuiButton-containedPrimary",
      "campusesDropdown": "#mui-component-select-campusOffices",
      "cannedResponseCount": "div p[data-testid='responseTitle']",
      "editButton": { "selector": "span", "text": "Edit" },
      "editTitle": "input[name='title']",
      "editResponse": "div.ck.ck-editor__main p"
    },
    "transcripts": {
      "readStatusButtonFirstRow": { "selector": "td>button[aria-label*='Mark']", "index": 0 },
      "exportTranscripts": "button[aria-label=' Export results to CSV']"
    }
  }
}